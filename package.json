{
  "name": "spotify-clone",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "yarn biome check src/",
    "lint:fix": "yarn biome check src/ --apply",
    "format": "yarn biome format src/ --write",
    "ci": "yarn biome ci src/",
    "test": "vitest run --coverage"
  },
  "dependencies": {
    "@biomejs/biome": "1.9.4",
    "@reduxjs/toolkit": "^2.6.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.8.1",
    "fast-average-color": "^9.4.0",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.27.0",
    "sass": "^1.85.1",
    "typescript": "^5.8.2",
    "universal-cookie": "^7.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.3",
    "jsdom": "^26.0.0",
    "vite": "^5.4.9",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "repository": "https://github.com/bastiendmt/spotify-like-web.git",
  "author": "Bastien DUMONT <bastienxs@gmail.com>",
  "license": "MIT"
}
